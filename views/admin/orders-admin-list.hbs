                                <tr>
                                    <th scope="col">Name</th>
                                    <th scope="col">Address</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">method</th>
                                    <th scope="col">Status</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each data}}
                                <tr>
                                    <td><b>{{this.address.firstname}}</b></td>
                                    <td>{{this.address.address}}</td>
                                    <td>${{this.products.0.sumTotal}}</td>
                                    <td><p id="date">{{this.deliveryDate}}</p></td>
                                    <td>{{this.paymentmethod}}</td>
                                    <td><span class="badge rounded-pill alert-warning">{{this.deliveryStatus}}</span></td>
                                    <td class="text-end">
                                    <a href="/order-product-details/{{this._id}}" class="btn btn-md rounded font-sm">Detail</a>
                                    </td>
                                </tr>
