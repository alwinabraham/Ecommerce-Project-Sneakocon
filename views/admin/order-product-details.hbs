                    </li>
                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <p>Lorem ipsum dolor sit amet.</p>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th scope="col">Image</th>
                                    <th scope="col">Productname</th>
                                    <th scope="col">Quantity</th>
                                    <th scope="col">sumtotal</th>
                                    <th scope="col">Delivery Date</th>
                                    <th scope="col">method</th>
                                    {{!-- <th scope="col">Status</th> --}}
                                    <th scope="col" class="text-end"> Status </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each products}}
                                <tr>
                                    <td class="product-thumbnail"> <img src="/uploads/{{this.products.image.[0]}}"  alt="" style="width: 100%px; height: 50px"class="img-fluid"></td>                                    
                                    <td>{{this.products.productName}}</td>
                                    <td>{{this.products.quantity}}</td>
                                    <td>${{this.products.sumTotal}}</td>
                                    <td><p id="date">{{this.deliveryDate}}</p></td>
                                    <td>{{this.paymentmethod}}</td>
                                    {{!-- <td><span class="badge rounded-pill alert-warning">{{this.products.productStatus}}</span></td> --}}
                                    <td class="text-end">
                                    <div class="dropdown">
                                            <select class="badge rounded-pill alert-warning" id="deliveryStatusChanger" name="deliveryStatus" onchange="return deliveryStatusChange('{{this._id}}','{{this.products.productid}}')">
                                                <option  value="{{this.products.productStatus}}">{{this.products.productStatus}}</option>
                                                <option value="Pending">Pending</option>
                                                <option value="Shipping">Shipping</option>
                                                <option value="Delivered">Delivered</option>
                                                <option value="Cancelled">Cancelled</option>
                                                <option value="Checking">Checking</option>
                                            </select>
                                    </div> 
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
