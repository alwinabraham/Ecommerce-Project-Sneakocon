    <script>
        function deliveryStatusChange(orderId,proId){
            let status = document.getElementById("deliveryStatusChanger").value;
            console.log(status)       
            $.ajax({
                type: 'POST',
                url: '/change-single-order-status',
                data: {
                    order: orderId,
                    product: proId,
                    status: status
                },
                success:(response)=> {}
            })
        }
    </script>
